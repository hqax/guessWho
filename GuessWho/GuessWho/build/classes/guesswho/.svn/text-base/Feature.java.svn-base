package guesswho;
/**
 *
 * @author Spencer
 */
public class Feature {

//name: beard
//parity: 2
//options = {true, false}
    private String name;
    private int parity;
    private String[] options;
    Feature(String n, String[] o){
        parity = o.length;
        name = n;
        options = o;
    }

    public String getName() {
        return name;
    }

    public String[] getOptions() {
        return options;
    }

    public int getParity() {
        return parity;
    }
    
    
}
