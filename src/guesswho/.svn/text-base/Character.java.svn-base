package guesswho;

import java.awt.*;
import java.util.ArrayList;

/**
 *
 * @author Spencer
 */
public class Character {

    private int column;
    private int row;
    private String name;
    private String filename;
    private ArrayList<String> features;
    
    Character(int c, int r, String n, String fn, ArrayList<String> feat) {
        column = c;
        row = r;
        name = n;
        filename = fn;
        features = feat;
        
    }
    
    public ArrayList<String> getFeatures() {
        return features;
    }
        
    public String getImageName() {
        return filename;
    }

    public String getName() {
        return name;
    }

}
   